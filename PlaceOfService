public class PLACEOFSERVICE{

     public static void main(String []args){
    
        int age= 70;
        char employee= 'M';
        char maritalStatus= 'Y';

         if(employee == 'F'){
                 System.out.println(" she will work only in urban areas ");
         }
         else if(age>20 && age<=40){
             System.out.println(" he may work in anywhere ");
         }
