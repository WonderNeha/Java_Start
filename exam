public class EXAM{

     public static void main(String []args){
        float NOC= 10;
        float NOCA= 9;
         if(NOCA*100/NOC >=75 ){
                 System.out.println("student is allowed to sit in exam");
         }
         else{
             System.out.println("student is not allowed to sit in exam");
         }
        
}
}
