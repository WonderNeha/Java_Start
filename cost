import java.util.Scanner;

public class cost{

     public static void main(String []args){
        int q = 4;
        int cost=q* 100;
        if(cost<=1000){
            System.out.println(" total cost of user "+cost);
        }
        else{
            System.out.println(" total cost of user "+cost*0.9);
        }
     }
}
