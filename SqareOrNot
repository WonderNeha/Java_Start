import java.util.Scanner;

public class SqareOrNot{

     public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int l = sc.nextInt();
        int b = l;
        
        if (l == b){
            System.out.println(" it is a square");
        }
        else{
            System.out.println("it is a rectangle");
        }
     }
}
